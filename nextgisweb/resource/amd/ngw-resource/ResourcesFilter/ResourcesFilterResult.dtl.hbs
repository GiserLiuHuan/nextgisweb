<div>
    <ul class="list-unstyled">
        {% for item in items %}
        <li data-resid="{{ item.resource.id }}">
            <a class="children-table__name__link withIcon withIcon-s" href="{{item.url}}">
                <svg class="icon">
                    <use xlink:href="#icon-rescls-{{item.resource.cls}}" />
                </svg>
                {{ item.resource.display_name }}
            </a>
        </li>
        {% endfor %}

        {% ifequal items.length 0 %}
        <li class="not-found">
            <a class="children-table__name__link" href="javascript:;">
                {{ tr.notFound }}
            </a>
        </li>
        {% endifequal %}
    </ul>
</div>
